{% extends 'base.html.twig' %}

{% block title %}Decoration{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>{{decoration.Titre}}</h1>
    <p>Date: {{decoration.Date}}</p>
    <p>Description:</p>
    <p>{{decoration.Description}}</p>
    <p>Image 1:</p>
    <p>{{decoration.Pic}}</p>
    <p>Image 2:</p>
    <p>{{decoration.Pic2}}</p>
    <p>Image 3:</p>
    <p>{{decoration.Pic3}}</p>
    <p>Image 4:</p>
    <p>{{decoration.Pic4}}</p>
</div>
{% endblock %}